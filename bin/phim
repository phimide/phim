#!/usr/bin/env php
<?php
/**
 * Usage:
 * 
 */
require_once __DIR__.'/../vendor/autoload.php';

$loader = new Composer\Autoload\ClassLoader();
$loader->add("Core", __DIR__ . "/../src");
$loader->add("Service", __DIR__ . "/../src");
$loader->register();

$bootstrap = new Core\Bootstrap(require __DIR__.'/../config/config.php');
$bootstrap->init();
